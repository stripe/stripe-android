  <string name="stripe_verification_message">Vaikimisi Linkiga maksmiseks sisestage numbrile %s saadetud kood.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Pole %s?</string>
