  <string name="wallet_recollect_cvc_error">โปรดป้อนรหัสรักษาความปลอดภัยของบัตรอีกครั้งเพื่อความปลอดภัย</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">แน่ใจไหมว่าต้องการนำบัญชีที่ลิงก์นี้ออก</string>
  <!-- Title for a button that when tapped removes a saved card. -->
  <string name="wallet_remove_card">นำบัตรออก</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="wallet_remove_card_confirmation">แน่ใจไหมว่าต้องการนำบัตรใบนี้ออก</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="wallet_remove_linked_account">นำบัญชีที่ลิงก์ไว้ออก</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="wallet_set_as_default">ตั้งเป็นค่าเริ่มต้น</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="wallet_unavailable">ใช้กับการซื้อรายการนี้ไม่ได้</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="wallet_update_card">อัปเดตบัตร</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="wallet_update_expired_card_error">บัตรใบนี้หมดอายุแล้ว โปรดอัปเดตข้อมูลบัตรหรือเลือกวิธีการชำระเงินอื่น</string>
</resources>
