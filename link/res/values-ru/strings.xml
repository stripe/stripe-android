  <string name="verification_message">Введите код, отправленный на адрес %s, для оплаты с помощью Link по умолчанию.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Не %s?</string>
