  <string name="wallet_bank_account_terms"><![CDATA[Ved å fortsette godtar du å autorisere betalinger i samsvar med <terms>vilkårene</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="wallet_collapsed_payment">Betaling</string>
