  <string name="verification_header_inline">Logg inn på Link-kontoen din</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_prefilled">Bruk den lagrede informasjonen din til å betale raskere</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="verification_message">Skriv inn koden sendt til %s for å bruke Link til å betale som standard.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Er du ikke %s?</string>
