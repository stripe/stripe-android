  <string name="wallet_recollect_cvc_error">For sikkerhets skyld må du skrive inn sikkerhetskoden for kortet ditt på nytt.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">Er du sikker på at du vil fjerne denne tilkoblede kontoen?</string>
