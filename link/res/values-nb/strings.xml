  <string name="wallet_default">Standard</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="wallet_expanded_title">Betalingsmetoder</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="wallet_pay_another_way">Betal på en annen måte</string>
