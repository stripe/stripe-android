  <string name="sign_up_message">Betal raskere hos %s og tusenvis av andre forhandlere.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="sign_up_terms"><![CDATA[Ved å bli med i Link, godtar du <terms>vilkårene</terms> og <privacy>personvernerklæringen</privacy>.]]></string>
