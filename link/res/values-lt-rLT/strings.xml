<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Pridėti banko sąskaitą</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Pridėkite mokėjimo būdą</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Išsaugokite savo informaciją saugiam atsiskaitymui vienu spustelėjimu su „Link“</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Atsijungti nuo „Link“</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Mokėti naudojant „Link“</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Nustatyti kaip numatytąjį mokėjimo būdą</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Rodyti meniu</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Prisijungti prie „Link“</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Saugus atsiskaitymas vienu spustelėjimu</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Mokėkite greičiau „%s“ ir daugybei kitų prekybininkų.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[Jungdamiesi prie Link sutinkate su <terms>Sąlygomis</terms> ir <privacy>Privatumo politika</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Pateikdami savo el. pašto adresą sutinkate susikurti „Link“ paskyrą bei įrašyti savo mokėjimo informaciją į „Link“ pagal <terms>„Link“ sąlygas</terms> ir <privacy>privatumo politiką</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[Pateikdami savo telefono numerį sutinkate susikurti „Link“ paskyrą bei įrašyti savo mokėjimo informaciją į „Link“ pagal <terms>„Link“ sąlygas</terms> ir <privacy>privatumo politiką</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">Pakeiskite el. paštą</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Kodas išsiųstas</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Įveskite patvirtinimo kodą</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Prisijunkite prie savo „Link“ paskyros</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Naudokite išsaugotą informaciją, kad atsiskaitytumėte greičiau</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">Įveskite kodą, išsiųstą numeriu %s, kad pagal numatytuosius nustatymus galėtumėte mokėti naudodami „Link“.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Ne %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Pakartotinai siųsti kodą</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[Tęsdami sutinkate autorizuoti mokėjimus pagal šias <terms>sąlygas</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Mokėjimas</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Numatytasis</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Mokėjimo būdai</string>
  <!-- Prefix for last 4 digits of payment method id e.g  •••• 1234 -->
  <string name="stripe_wallet_last4_prefix">••••</string>
  <!-- Accessibility description for Passthrough payment method -->
  <string name="stripe_wallet_passthrough_description">Perdavimas</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Mokėkite kitu būdu</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Saugumo sumetimais dar kartą įveskite kortelės saugumo kodą.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Ar tikrai norite pašalinti šią susietą paskyrą?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Ar tikrai norite pašalinti šią kortelę?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Pašalinti susietą paskyrą</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Nustatyti kaip numatytąjį</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Šiam pirkiniui nepasiekiama</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Atnaujinti kortelę</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Ši kortelė nebegalioja. Atnaujinkite savo kortelės informaciją arba pasirinkite kitą mokėjimo būdą.</string>
</resources>
