<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Tilføj bankkonto</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Tilføj en betalingsmetode</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Gem dine oplysninger om sikker betaling med 1 klik med Link</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Log ud af Link</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Betal med Link</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Angiv som standardbetalingsmetode</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Vis menu</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Tilmeld Link</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Sikker 1-kliks-betaling</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Betal hurtigere hos %s og tusinder af forhandlere.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[Ved at tilmelde dig Link accepterer du <terms>vilkårene</terms> og <privacy>privatlivspolitikken</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Når du angiver din e-mail, accepterer du at oprette en Link-konto og gemme dine betalingsoplysninger på Link i henhold til <terms>Links Vilkår</terms> og <privacy>Privatlivspolitik</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[Når du angiver dit telefonnummer, accepterer du at oprette en Link-konto og gemme dine betalingsoplysninger på Link i henhold til <terms>Links Vilkår</terms> og <privacy>Privatlivspolitik</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">Skift e-mail</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Kode sendt</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Indtast din bekræftelseskode</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Log ind på din Link-konto</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Brug dine gemte oplysninger til hurtigere betaling</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">Indtast koden, der blev sendt til %s, for at bruge Link som standardbetaling.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Ikke %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Send kode igen</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[Ved at fortsætte accepterer du at godkende betalinger i henhold til disse <terms>vilkår</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Betaling</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Standard</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Betalingsmetoder</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Betal på en anden måde</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Af hensyn til sikkerheden skal du indtaste dit korts sikkerhedskode igen.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Er du sikker på, at du vil fjerne denne linket konto?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Er du sikker på, at du vil fjerne dette kort?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Fjern linket konto</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Brug som standard</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Ikke tilgængelig for dette køb</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Opdater kort</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Dette kort er udløbet. Opdater dine kortoplysninger eller vælg en anden betalingsmetode.</string>
</resources>
