<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Adicionar conta bancária</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Adicionar um método de pagamento</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Guardar as suas informações para finalização de compra de 1 clique com Link</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Terminar sessão no Link</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Pagar com Link</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Definir como método de pagamento predefinido</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Mostrar menu</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Aderir ao Link</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Finalização de compra segura de 1 clique</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Pague mais rapidamente em %s e milhares de comerciantes.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[Ao juntar-se ao Link, concorda com as <terms>Condições</terms> e com a <privacy>Política de Privacidade</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Ao fornecer o seu email, concorda em criar uma conta Link e guardar as suas informações de pagamento no Link, de acordo com as <terms>condições do Link</terms> e a <privacy>política de privacidade</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[Ao fornecer o seu número de telefone, concorda em criar uma conta Link e guardar as suas informações de pagamento no Link, de acordo com as <terms>condições do Link</terms> e a <privacy>política de privacidade</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">Alterar email</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Código enviado</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Introduza o seu código de verificação</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Inicie sessão na sua conta Link</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Utilizar as suas informações guardadas para finalizar compras mais rapidamente</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">Introduza o código enviado para %s para utilizar o Link como método de pagamento predefinido.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Não é %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Enviar novamente o código</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[Ao continuar, concorda em autorizar pagamentos de acordo com estes <terms>termos</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Pagamento</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Predefinido</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Métodos de pagamento</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Pagar de outra forma</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Por motivos de segurança, volte a introduzir o código de segurança do seu cartão.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Tem a certeza de que quer remover esta conta associada?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Tem a certeza de que quer remover este cartão?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Remover conta associada</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Tornar predefinido</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Indisponível para esta compra</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Atualizar cartão</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Este cartão expirou. Atualize as informações do seu cartão ou utilize um método de pagamento diferente.</string>
</resources>
