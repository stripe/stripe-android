<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="add_bank_account">Dodajte bankovni račun</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="add_payment_method">Dodaj način plaćanja</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="inline_sign_up_header">Sačuvaj moje podatke za sigurno plaćanje pomoću 1 klika</string>
  <!-- Title of the logout action. -->
  <string name="log_out">Odjavite se s Linka</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="pm_set_as_default">Postavi kao zadani način plaćanja</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="show_menu">Prikaži izbornik</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="sign_up">Pridružite se na Link</string>
  <!-- Title for the Link signup screen -->
  <string name="sign_up_header">Sigurno plaćanje pomoću 1 klika</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="sign_up_message">Platite brže kod %s i tisuća drugih trgovaca.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="sign_up_terms"><![CDATA[Prisupanjem u Link, prihvaćate <terms>Uvjete</terms> i <privacy>Pravila privatnosti</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="verification_change_email">Izmijeni e-poštu</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header">Unesite verifikacijski kod</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_inline">Prijavite se na svoj Link račun</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_prefilled">Upotrijebite svoje spremljene podatke za brže plaćanje</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="verification_message">Unesite kod poslan na %s kako biste koristili Link za plaćanje kao zadanu opciju.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Niste %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="verification_resend">Ponovo pošalji kod</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="wallet_bank_account_terms"><![CDATA[Ako nastavite, pristajete na autorizaciju plaćanja u skladu s ovim <terms>uvjetima</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="wallet_collapsed_payment">Plaćanje</string>
  <!-- Label for identifying the default payment method. -->
  <string name="wallet_default">Zadano</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="wallet_expanded_title">Načini plaćanja</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="wallet_pay_another_way">Platite na drugi način</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="wallet_recollect_cvc_error">Radi sigurnosti, ponovno unesite sigurnosni kod vaše kartice.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">Jeste li sigurni da želite ukloniti ovaj povezani račun?</string>
  <!-- Title for a button that when tapped removes a saved card. -->
  <string name="wallet_remove_card">Ukloni karticu</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="wallet_remove_card_confirmation">Jeste li sigurni da želite ukloniti ovu karticu?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="wallet_remove_linked_account">Ukloni povezani račun</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="wallet_set_as_default">Postavi kao zadano</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="wallet_unavailable">Nije dostupno za ovu kupnju</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="wallet_update_card">Ažuriraj karticu</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="wallet_update_expired_card_error">Ova kartica je istekla. Ažurirajte svoje podatke o kartici ili odaberite drugi način plaćanja.</string>
</resources>
