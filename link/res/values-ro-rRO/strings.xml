  <string name="sign_up_message">Plătiți mai rapid la %s și alți mii de comercianți.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="sign_up_terms"><![CDATA[Prin accesarea link-ului de asociere, sunteți de acord cu <terms>Termenii</terms> și <privacy>Politica de confidențialitate</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="verification_change_email">Schimbați adresa de e-mail</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header">Introduceți codul de verificare</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_inline">Conectați-vă la contul dvs. Link</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_prefilled">Folosiți informațiile dvs. salvate pentru a finaliza comanda mai rapid</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="verification_message">Introduceți codul trimis la %s pentru a utiliza Link ca metodă de plată implicită</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Nu este %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="verification_resend">Retrimitere cod</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="wallet_bank_account_terms"><![CDATA[Continuând, sunteți de acord să autorizați plățile în conformitate cu acești <terms>termeni</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="wallet_collapsed_payment">Plată</string>
  <!-- Label for identifying the default payment method. -->
  <string name="wallet_default">Implicit</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="wallet_expanded_title">Metode de plată</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="wallet_pay_another_way">Folosiți altă metodă de plată</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="wallet_recollect_cvc_error">Din motive de securitate, vă rugăm să introduceți din nou codul de securitate al cardului dvs.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">Sigur doriți să eliminați acest cont asociat?</string>
  <!-- Title for a button that when tapped removes a saved card. -->
  <string name="wallet_remove_card">Eliminare card</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="wallet_remove_card_confirmation">Sigur doriți să eliminați acest card?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="wallet_remove_linked_account">Eliminare cont asociat</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="wallet_set_as_default">Setați ca implicită</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="wallet_unavailable">Nu este disponibilă pentru această achiziție</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="wallet_update_card">Actualizare card</string>
