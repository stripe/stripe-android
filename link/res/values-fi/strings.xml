  <string name="verification_header_inline">Kirjaudu sisään Link-tiliisi</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_prefilled">Käytä tallennettuja tietoja nopeampaan maksamiseen</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="verification_message">Syötä numeroon %s lähetetty koodi käyttääksesi Link-maksua oletuksena.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Ei %s?</string>
