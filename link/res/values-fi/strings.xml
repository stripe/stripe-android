  <string name="wallet_collapsed_payment">Maksu</string>
  <!-- Label for identifying the default payment method. -->
  <string name="wallet_default">Oletus</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="wallet_expanded_title">Maksutavat</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="wallet_pay_another_way">Maksa toisella tavalla</string>
