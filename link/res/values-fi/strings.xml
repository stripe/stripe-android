<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Lisää pankkitili</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Lisää maksutapa</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Tallenna tiedot turvalliselle yhden napsautuksen maksamiselle Link-palvelulla</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Kirjaudu ulos Link-palvelusta</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Maksa Link-palvelulla</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Aseta oletusarvoiseksi maksutavaksi</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Näytä valikko</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Liity Link-palveluun</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Turvallinen kerran napsautettava maksaminen</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Maksa nopeammin palvelussa %s ja tuhansilla muilla kauppiailla.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[Liittymällä Linkiin hyväksyt <terms>ehdot</terms> ja <privacy>tietosuojaselosteen</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Antamalla sähköpostisi hyväksyt Link-tilin luomisen ja tallennat maksutietosi Link-palveluun <terms>Link-ehtojen</terms> ja <privacy>tietosuojaselosteen</privacy> mukaisesti.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[Antamalla puhelinnumerosi hyväksyt Link-tilin luomisen ja tallennat maksutietosi Link-palveluun <terms>Link-ehtojen</terms> ja <privacy>tietosuojaselosteen</privacy> mukaisesti.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">Vaihda sähköposti</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Koodi lähetetty</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Syötä vahvistuskoodi</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Kirjaudu sisään Link-tiliisi</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Käytä tallennettuja tietoja nopeampaan maksamiseen</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">Syötä numeroon %s lähetetty koodi käyttääksesi Link-maksua oletuksena.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Ei %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Lähetä koodi uudelleen</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[Jatkamalla hyväksyt maksut näiden <terms>ehtojen</terms> mukaisesti.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Maksu</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Oletus</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Maksutavat</string>
  <!-- Prefix for last 4 digits of payment method id e.g  •••• 1234 -->
  <string name="stripe_wallet_last4_prefix">•••• </string>
  <!-- Accessibility description for Passthrough payment method -->
  <string name="stripe_wallet_passthrough_description">Passthrough</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Maksa toisella tavalla</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Turvallisuussyistä sinun on annettava korttisi turvakoodi uudelleen.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Haluatko varmasti poistaa tämän linkitetyn tilin?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Haluatko varmasti poistaa tämän kortin?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Poista linkitetty tili</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Aseta oletusarvoiseksi</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Ei käytettävissä tälle ostolle</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Päivitä kortti</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Tämä kortti on erääntynyt. Päivitä kortin tiedot tai valitse toinen maksutapa.</string>
</resources>
