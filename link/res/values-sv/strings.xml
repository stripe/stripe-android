<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Lägg till bankkonto</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Lägg till en betalningsmetod</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Spara dina uppgifter för säker betalning med ett klick med Link</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Logga ut från Link</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Betala med Link</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Ställ in som standardbetalningsmetod</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Visa meny</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Börja använda Link</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Betala säkert med ett klick</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Betala snabbare hos %s och hos tusentals handlare.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[Genom att registrera dig på Link, godkänner du våra <terms>villkor</terms> och vår <privacy>integritetspolicy</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Genom att tillhandahålla din e-postadress godkänner du att skapa ett Link-konto och spara dina betalningsuppgifter hos Link, i enlighet med <terms>Links villkor</terms> och <privacy>integritetspolicy</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[Genom att tillhandahålla ditt telefonnummer godkänner du att skapa ett Link-konto och spara dina betalningsuppgifter hos Link, i enlighet med <terms>Links villkor</terms> och <privacy>integritetspolicy</privacy>.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">Ändra e-postadress</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Koden har skickats</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Ange din verifieringskod</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Logga in på ditt Link-konto</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Använd din sparade information för att betala snabbare</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">Ange koden som skickades till %s för att använda Link för att betala som standard.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Inte %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Skicka koden igen</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[Genom att fortsätta samtycker du till att godkänna betalningar i enlighet med dessa <terms>villkor</terms>.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Betalning</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Standard</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Betalningsmetoder</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Betala på ett annat sätt</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Ange säkerhetskoden för ditt kort igen av säkerhetsskäl.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Är du säker på att du vill ta bort det länkade kontot?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Är du säker på att du vill ta bort kortet?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Ta bort länkat konto</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Ställ in som standard</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Otillgängligt för detta köp</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Uppdatera kort</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Detta kort har löpt ut. Uppdatera kortinformationen eller använd en annan betalningsmetod.</string>
</resources>
