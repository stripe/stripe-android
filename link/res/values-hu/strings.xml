<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!-- Button prompt to add a bank account as a payment method. -->
  <string name="stripe_add_bank_account">Bankszámla hozzáadása</string>
  <!-- Text for a button that, when tapped, displays another screen where the customer can add a new payment method -->
  <string name="stripe_add_payment_method">Fizetési mód hozzáadása</string>
  <!-- Label for a checkbox that when checked allows the payment information to be saved and used in future checkout sessions. -->
  <string name="stripe_inline_sign_up_header">Mentse el az adatait a biztonságos egykattintásos fizetéshez a Linkkel</string>
  <!-- Title of the logout action. -->
  <string name="stripe_log_out">Kijelentkezés a Link</string>
  <!-- Text for the 'Pay with Link' button. 'Link' is a Stripe brand, please do not translate the word 'Link'. -->
  <string name="stripe_pay_with_link">Fizetés Linkkel</string>
  <!-- Label of a checkbox that when checked makes a payment method as the default one. -->
  <string name="stripe_pm_set_as_default">Legyen ez az alapértelmezett fizetési mód</string>
  <!-- Accessibility label for an action or a button that shows a menu. -->
  <string name="stripe_show_menu">Menü megjelenítése</string>
  <!-- Title for a button that when tapped creates a Link account for the user. -->
  <string name="stripe_sign_up">Csatlakozás a Linkhez</string>
  <!-- Title for the Link signup screen -->
  <string name="stripe_sign_up_header">Biztonságos egykattintásos fizetés</string>
  <!-- Label describing the benefit of signing up for Link. Pay faster at {Merchant Name} and thousands of merchants. e.g, 'Pay faster at Example, Inc. and thousands of merchants.' -->
  <string name="stripe_sign_up_message">Gyorsabban fizethet a(z) %s számára és még ezernyi kereskedőnek.</string>
  <!-- Legal text shown when creating a Link account. -->
  <string name="stripe_sign_up_terms"><![CDATA[A Linkhez történő csatlakozással Őn elfogadja a <terms>feltételeket</terms> és az <privacy>adatvédelmi szabályzatot</privacy>.]]></string>
  <!-- Legal text shown when creating a Link account in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative"><![CDATA[Az e-mail-címe megadásával hozzájárul egy Link fiók létrehozásához és a fizetési adatai mentéséhez a Linkbe, a <terms>Link feltételei</terms> és <privacy>adatvédelmi szabályzata</privacy> szerint.]]></string>
  <!-- Legal text shown when creating a Link account with a phone number in the alternative flow. -->
  <string name="stripe_sign_up_terms_alternative_with_phone_number"><![CDATA[A telefonszáma megadásával hozzájárul egy Link fiók létrehozásához és a fizetési adatai mentéséhez a Linkbe, a Link <terms>feltételei</terms> és <privacy>adatvédelmi szabályzata</privacy> szerint.]]></string>
  <!-- Title for a button that allows the user to use a different email in the signup flow. -->
  <string name="stripe_verification_change_email">E-mail-cím módosítása</string>
  <!-- Text of a notification shown to the user when a login code is successfully sent via SMS. -->
  <string name="stripe_verification_code_sent">Kód elküldve</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header">Adja meg az ellenőrző kódot</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_inline">Bejelentkezés a Link-fiókba</string>
  <!-- Two factor authentication screen heading -->
  <string name="stripe_verification_header_prefilled">Mentett adatok használata a gyorsabb fizetéshez</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="stripe_verification_message">A Link alapértelmezett fizetési módként történő használatához adja meg a(z) %s számra küldött kódot</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="stripe_verification_not_email">Nem a(z) %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="stripe_verification_resend">Kód újraküldése</string>
  <!-- Mandate text displayed when paying via Link instant debit. -->
  <string name="stripe_wallet_bank_account_terms"><![CDATA[A folytatással elfogadja, hogy engedélyezi a kifizetéseket ezen <terms>feltételek</terms> szerint.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="stripe_wallet_collapsed_payment">Fizetés</string>
  <!-- Label for identifying the default payment method. -->
  <string name="stripe_wallet_default">Alapértelmezett</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="stripe_wallet_expanded_title">Fizetési módok</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="stripe_wallet_pay_another_way">Fizetés más módon</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="stripe_wallet_recollect_cvc_error">Biztonsági okból adja meg újra kártyája biztonsági kódját.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="stripe_wallet_remove_account_confirmation">Biztosan törli ezt a kapcsolt számlát?</string>
  <!-- Title of confirmation prompt when removing a saved card. -->
  <string name="stripe_wallet_remove_card_confirmation">Biztosan törli ezt a kártyát?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="stripe_wallet_remove_linked_account">Kapcsolt számla törlése</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="stripe_wallet_set_as_default">Legyen alapértelmezett</string>
  <!-- Label shown when a payment method cannot be used for the current transaction. -->
  <string name="stripe_wallet_unavailable">Nem érhető el ennél a vásárlásnál</string>
  <!-- Title for a button that when tapped, presents a screen for updating a card. Also the heading the screen itself. -->
  <string name="stripe_wallet_update_card">Kártya módosítása</string>
  <!-- A text notice shown when the user selects an expired card. -->
  <string name="stripe_wallet_update_expired_card_error">Ez a kártya lejárt. Frissítse a kártyájat, vagy használjon másik fizetési módot.</string>
</resources>
