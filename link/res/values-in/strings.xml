  <string name="wallet_remove_card_confirmation">Anda yakin ingin menghapus kartu ini?</string>
  <!-- Title for a button that when tapped removes a linked bank account. -->
  <string name="wallet_remove_linked_account">Hapus rekening tertaut</string>
  <!-- Label for a button or menu item that sets a payment method as default when tapped. -->
  <string name="wallet_set_as_default">Atur sebagai default</string>
