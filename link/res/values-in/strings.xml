  <string name="verification_header_inline">Masuk ke akun Link Anda</string>
  <!-- Two factor authentication screen heading -->
  <string name="verification_header_prefilled">Gunakan info yang tersimpan untuk memproses pembayaran dengan lebih cepat</string>
  <!-- Instructs the user to enter the code sent to their phone number in order to login to Link -->
  <string name="verification_message">Masukkan kode yang dikirim ke %s untuk menggunakan Link untuk membayar secara default.</string>
  <!-- Text of a label for confirming an email address. E.g., 'Not user@example.com?' -->
  <string name="verification_not_email">Bukan %s?</string>
  <!-- Label for a button that re-sends the a login code when tapped -->
  <string name="verification_resend">Kirim ulang kode</string>
