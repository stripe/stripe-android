  <string name="wallet_bank_account_terms"><![CDATA[Dengan melanjutkan, berarti Anda setuju untuk mengotorisasi pembayaran sesuai dengan <terms>ketentuan</terms> ini.]]></string>
  <!-- Label for a section displaying payment details. -->
  <string name="wallet_collapsed_payment">Pembayaran</string>
  <!-- Label for identifying the default payment method. -->
  <string name="wallet_default">Default</string>
  <!-- Title for a section listing one or more payment methods. -->
  <string name="wallet_expanded_title">Metode pembayaran</string>
  <!-- Label of a button that when tapped allows the user to select a different form of payment. -->
  <string name="wallet_pay_another_way">Bayar dengan cara lain</string>
  <!-- A text notice shown when the user selects a card that requires re-entering the security code (CVV/CVC). -->
  <string name="wallet_recollect_cvc_error">Untuk keamanan, masukkan lagi kode keamanan kartu Anda.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">Anda yakin ingin menghapus rekening tertaut ini?</string>
  <!-- Title for a button that when tapped removes a saved card. -->
  <string name="wallet_remove_card">Hapus kartu</string>
