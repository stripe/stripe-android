  <string name="wallet_recollect_cvc_error">For security, please re-enter your card’s security code.</string>
  <!-- Title of confirmation prompt when removing a linked bank account. -->
  <string name="wallet_remove_account_confirmation">Are you sure you want to remove this linked account?</string>
  <!-- Title for a button that when tapped removes a saved card. -->
  <string name="wallet_remove_card">Remove card</string>
